<%= form_for note do |f| %>
    <%= f.label "date" %>
    <%= f.date_select(:date, :order => [:month, :day, :year]) %> 
    <br>
    <br>
    <%= f.label "Rating" %>
    <%= f.number_field :rating %>
    <br>
    <br>
    <%= f.label "Vendor" %>
    <%= f.text_field :vendor %>
    <br>
    <br>
    <%= f.hidden_field :collection_id, value: collection_id %> 
    
    <%= f.label "Note" %>
    <%= f.text_area :note %>
    <%= f.submit %>
<% end %>