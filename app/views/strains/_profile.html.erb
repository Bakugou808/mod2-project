<h1>Strain Profile</h1>
<br>
<br>
<h2><%= strain.name %></h2>
<br>
<h2> 
    <ul>
        
            
            <br>
            <br>
            Type: <%= strain.genus.capitalize %>
            <br>
            <br>
            <%= strain.description %>
            <br>
            <br>
            Effects: 
            <br>
            <%= effects[0] %>
            <br>
            <%= effects[1] %>
            <br>
            <%= effects[2] %>
            <br>
            <br>
            <br>
            Flavors: 
            <br><%= strain.flavors %>
            <br>
            <br>
            Chemical Profile: 
            <br>
            <br>
                <% strain.lab_results[0].terps.sort.each do |terp, val| %>
                    <li><%= "#{terp.capitalize}: #{val}" %></li>
                    <br>
                <% end %>                
                <br>
    </ul>
</h2>